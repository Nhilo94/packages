{
  "name": "@adriantombu/paybox-system",
  "version": "1.0.6",
  "description": "A simple implementation of the Paybox System payment solution",
  "keywords": [
    "paybox",
    "form",
    "payment",
    "gateway",
    "e-commerce"
  ],
  "main": "lib/paybox.js",
  "repository": "https://github.com/adriantombu/paybox-direct",
  "author": "Adrian Tombu <adrian@otso.fr>",
