  "author": "Adrian Tombu <adrian@otso.fr> (https://otso.fr)",
  "license": "MIT",
  "devDependencies": {
    "@types/pkgcloud": "^1.7.3",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "pkgcloud": "^2.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
